pip install marshmallow-sqlalchemy>=1.0


from sqlalchemy import create_engine
engine = create_engine("oracle+oracledb://user1:pass1@host1:port1/service1")
with engine.connect() as conn:
    result = conn.execute("SELECT 1 FROM DUAL").fetchall()
    print(result)


